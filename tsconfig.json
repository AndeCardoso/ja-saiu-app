{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["./app/components/*"],
      "@pages/*": ["./app/pages/*"],
      "@global/*": ["./app/global/*"],
      "@assets/*": ["./app/assets/*"],
      "@utils/*": ["./app/utils/*"],
      "@services/*": ["./infra/services/*"],
      "@http/*": ["./infra/http/*"],
      "@model/*": ["./model/*"],
      "@tools/*": ["./tools/*"],
    }
  },
}
